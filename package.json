{
  "name": "fapi-connector",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "build": "tsc && json-dereference -s ./openapi.yaml -o ./gen/openapi.deref.yaml",
    "serve": "NODE_ENV=production yarn run build",
    "start": "yarn build && node lib/server.js",
    "test": "yarn build && ENV_FILE=/.config/.env.test jest --detectOpenHandles"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "main": "lib/server.js",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@dextersjab/openid-client": "^0.1.1",
    "@prisma/client": "^4.4.0",
    "@types/crypto-js": "^4.1.1",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "axios": "1.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-json-validator-middleware": "^3.0.1",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "jose": "^4.10.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lru-cache": "^7.14.0",
    "morgan": "^1.10.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prisma": "^4.4.0",
