  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^29.1.2",
    "@types/js-yaml": "^4.0.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
